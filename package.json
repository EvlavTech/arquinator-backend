{
  "name": "arquinator-backend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/EvlavTech/arquinator-backend",
  "author": "Evlav Tech",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "yarn clean && tsc",
    "start": "node dist/index.js",
    "migrate": "yarn sequelize db:migrate --env development",
    "migrate:undo": "yarn sequelize db:migrate:undo --env development",
    "migrate:undo:all": "yarn sequelize db:migrate:undo:all --env development",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^14.14.0",
    "@types/sequelize": "^4.28.9",
    "nodemon": "^2.0.6",
    "sequelize-cli": "^6.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.4.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5"
  }
}
